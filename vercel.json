{
    "version": 2,
    "rewrites": [
        {
          "source": "/api/voxcpm/(.*)",
          "destination": "https://mouth-three-edinburgh-expo.trycloudflare.com/$1"
        }
      ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
          { "key": "X-Content-Type-Options", "value": "nosniff" }
        ]
      }
    ]
  }
  