{
    "version": 2,
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "${NEXT_PUBLIC_API_BASE}/$1"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
          { "key": "X-Content-Type-Options", "value": "nosniff" }
        ]
      }
    ]
  }
  